class Solution(object):
    def plusOne(self, digits):
        """
        :type digits: List[int]
        :rtype: List[int]
        """
        s=""
        for i in digits:
            s+=str(i)
        num=str(int(s)+1)
        digits=[]
        for i in range(len(num)):
            digits.append(int(num[i]))
        return digits
